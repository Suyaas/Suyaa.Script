namespace(Su).notes("苏语言")

class(Example).notes("示例类")

    function(Main).notes("主函数")
        define(id:int).notes("标识符")
        set(id, Add(1, 2))
        define(cls:MyClass).notes("自定义类")
        set(cls, call(MyClass.instance).parameters(id, "My Name").invoke())
        call(System.Console.WriteLine).parameters(cls.GetName()).invoke()

    function(Add:int).private().notes("计算算术和")
        parameter(a:int).notes("加数")
        parameter(b:int).notes("被加数")
        define(c:int).notes("算术和")
        set(c, !(a + b))
        return(c)

# 定义一个对象
class(MyClass).public()

    # 定义变量
    define(
        _id:int, 
        _name:string,
    ).private()

    # 定义构造函数
    function().public().parameters(id:int, name:string)
        set(_id, id)
        set(_name, name)
    
    # 获取名称
    function(GetName:string).public()
        return(_name)

    # 设置名称
    function(SetName).public().parameters(name:string)
        set(_name, name)